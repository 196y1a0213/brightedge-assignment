{
    "name": "brightedge/assignment",
    "description": "Brightedge Assignment - Modular PHP Application",
    "type": "project",
    "require": {
        "php": ">=7.4",
        "vlucas/phpdotenv": "^5.5",
        "guzzlehttp/guzzle": "^7.0",
        "symfony/dom-crawler": "^5.4|^6.0",
        "symfony/css-selector": "^5.4|^6.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5"
    },
    "autoload": {
        "psr-4": {
            "App\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "config": {
        "optimize-autoloader": true,
        "sort-packages": true
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}

